<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
            android:id="@+id/navigation_home"
            android:name="kr.co.uxn.agms.ui.home.NewHomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_dashboard" />

    <fragment
        android:id="@+id/navigation_event"
        android:name="kr.co.uxn.agms.ui.event.EventFragment"
        android:label="@string/title_diary"
        tools:layout="@layout/fragment_event" />


    <fragment
        android:id="@+id/navigation_setting"
        android:name="kr.co.uxn.agms.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/fragment_dashboard" />

</navigation>